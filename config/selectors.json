{
    "display_input": [
        "span + [role=\"button\"]",
        "[aria-label*=\"Write something\"]",
        "//span[contains(text(), 'Write something')]"
    ],
    "input": [
        "div.notranslate._5rpu[role=\"textbox\"]",
        "//div[@role='textbox' and contains(@class, 'notranslate')]"
    ],
    "display_themes": [
        "div[aria-label*=\"Show Background Options\"]",
        "//div[contains(@aria-label, 'Show Background Options')]"
    ],
    "theme": [
        "div.x1qjc9v5.x78zum5.x1q0g3np.xozqiw3.xcud41i.x139jcc6.x1n2onr6.xl56j7k > div:nth-child(index) > div[aria-pressed=\"false\"]",
        "//div[contains(@class, 'x1qjc9v5') and .//div[@aria-pressed='false']]"
    ],
    "show_image_input": [
        "[aria-label*=\"Photo/video\"]",
        "//*[contains(@aria-label, 'Photo/video')]"
    ],
    "add_image": [
        "input[type=\"file\"][accept^=\"image/*\"]",
        "//input[@type='file' and starts-with(@accept, 'image/*')]"
    ],
    "submit": [
        "[aria-label*=\"Post\"][role=\"button\"]",
        "//*[contains(@aria-label, 'Post') and @role='button']"
    ],
    "comment_input": [
        "div[aria-label='Write a comment']",
        "//div[@aria-label='Write a comment']"
    ],
    "submit_comment": [
        "div[aria-label='Post comment']",
        "//div[@aria-label='Post comment']"
    ],
    "group_link": [
        "a[role='link'][href*='/groups/']",
        "//a[@role='link' and contains(@href, '/groups/')]"
    ]
}
